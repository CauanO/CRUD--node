<div class="categories-page">
    <h2>Lista de Postagens</h2>
    <hr>
    <a href="/adm/postagens/add"><button type="button" class="btn btn-primary">Postar</button></a>
</div>

{{#each postagens}}
<div class="card mt-4">
    <div class="body-card">
        <h4>Titulo: {{titulo}}</h4>
        <p>Descrição: {{descricao}}</p>
        <small>Data: {{data}}</small>
        <small>Categoria: {{categoria.nome}}</small>
    </div>
</div>
{{else}}
{{/each}}